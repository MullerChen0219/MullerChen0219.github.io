function tabInit(){resetInterval(2);var t=0;$(".sl-tab").find("button").on("click",function(){t=$(this).index()+1,$(this).addClass("active").siblings().removeClass("active"),$(".sl-img").attr("data-open",t),resetInterval(t+1)})}function resetInterval(t){clearInterval(tabInterval),tabInterval=setInterval(function(){4==t&&(t=1),$(".sl-img, .sl-animate-w").attr("data-open",t),t++},4e3)}function owlInit(){$(".sl-command-list").owlCarousel({loop:!0,items:1,navText:["",""],responsiveClass:!0,responsive:{0:{nav:!0,dots:!0},768:{nav:!1}}})}$(function(){owlInit(),tabInit()});var tabInterval="";